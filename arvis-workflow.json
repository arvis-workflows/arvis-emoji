{
  "$schema": "https://github.com/jopemachine/arvis-core/blob/master/workflow-schema.json",
  "category": "Productivity",
  "creator": "Sindre Sorhus",
  "name": "Emoj",
  "readme": "",
  "enabled": true,
  "defaultIcon": "icon.png",
  "commands": [
    {
      "type": "scriptFilter",
      "command": "emoj",
      "title": "Find relevant emoji",
      "subtitle": "",
      "scriptFilter": "node index.js \"$1\" PUT_YOUR_API_KEY_HERE",
      "runningSubtext": "Searching...",
      "withspace": true,
      "arg_type": "required",
      "actions": [
        {
          "modifiers": "normal",
          "type": "clipboard",
          "text": "{query}"
        }
      ]
    }
  ]
}
